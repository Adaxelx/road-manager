{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev-mock": "set NODE_ENV=mock && vite",
        "update-mocks": "npx msw init public --no-save",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "openapi-generate": "rimraf src/api & openapi-generator-cli generate -i openapi/swagger.yaml -g typescript-fetch -o src/api/ --additional-properties=stringEnums & npm run openapi-lint",
        "openapi-lint": "npx prettier --write src/api"
    },
    "dependencies": {
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@mui/icons-material": "^5.10.9",
        "@mui/material": "^5.10.12",
        "@openapitools/openapi-generator-cli": "^2.5.2",
        "formik": "^2.2.9",
        "leaflet": "^1.9.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-leaflet": "^4.1.0",
        "react-router-dom": "^6.4.3",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@types/leaflet": "^1.9.0",
        "@types/react": "^18.0.22",
        "@types/react-dom": "^18.0.7",
        "@vitejs/plugin-react": "^2.2.0",
        "msw": "^0.48.0",
        "prettier": "2.7.1",
        "rimraf": "^3.0.2",
        "sass": "^1.56.0",
        "typescript": "^4.6.4",
        "vite": "^3.2.0"
    }
}
